---
import { Image } from "astro:assets";

type Props = {
	// biome-ignore lint/suspicious/noExplicitAny: <explanation>
	src: any;
	alt: string;
	// biome-ignore lint/suspicious/noExplicitAny: <explanation>
	[key: string]: any;
};

const { src, alt, ...props } = Astro.props;
---

<Image src={src} alt={alt} {...props} transition:name={alt} transition:animate="initial" />
